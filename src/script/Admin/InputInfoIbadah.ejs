      <div class="col-sm-10 mt-2 pr-3 pt-4">
        <h3 class="mb-4" style="text-align: center;">INFORMASI IBADAH SEPEKAN</h3>

        <form action="/InputInfoIbadah" method="post">
          <div class="mb-4">
            <label for="JudulIbadah" class="form-label">Judul Ibadah </label>
            <input type="text" class="form-control" name="JudulIbadah" id="formGroupExampleInput" placeholder="Masukkan Judul Ibadah" />
          </div>
          <div class="mb-4">
            <label for="TanggalIbadah" class="form-label">Tanggal Ibadah</label>
            <input type="text" class="form-control" name="TanggalIbadah" id="datepicker" placeholder="Masukkan Tanggal Ibadah" />
            
          </div>
          <div class="mb-4">
            <label for="Lokasi" class="form-label">Lokasi Ibadah</label>
            <input type="text" class="form-control" name="Lokasi" id="formGroupExampleInput2" placeholder="Masukkan Lokasi Ibadah" />
          </div>

          <button type="submit" class="btn btn-outline-primary mt-3">Upload</button>
        </form>
      </div>
